<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog">
    <defaultcontrol>200</defaultcontrol>
    
    <controls>
        <control type="button" id="200">
            <left>800</left>
            <top>500</top>
            <width>320</width>
            <height>80</height>
            <label>Launch Dialog</label>
            <onclick>RunScript(script.skinshortcuts,type=manage&amp;group=autowidget-$INFO[Window.Property(dialog.group)])</onclick>
            <onunload>RunScript(script.skinshortcuts,type=buildxml&amp;mainmenuID=9000&amp;group=$INFO[Window().Property(dialog.group_list)])</onunload>
        </control>
    </controls>
</window>