<addons><addon id="plugin.program.autowidget" name="AutoWidget" provider-name="drinfernoo" version="0.0.51">
    <requires>
        <import addon="xbmc.python" version="2.25.0" />
        <import addon="script.module.six" />
        <import addon="script.skinshortcuts" />
    </requires>
    <extension library="main.py" point="xbmc.python.pluginsource">
        <provides>executable</provides>
    </extension>
    <extension library="service.py" point="xbmc.service" />
    <extension point="xbmc.addon.metadata">
        <summary lang="en_GB">Automatically changes out widgets on the homescreen for a more interesting browsing experience.</summary>
        <description lang="en_GB" />
        <language />
        <platform>all</platform>
        <license>LGPL-3.0</license>
        <forum />
        <website>https://www.github.com/drinfernoo/</website>
        <email />
        <source />
        <news />
        <disclaimer />
        <assets>
            <icon>resources/icon.png</icon>
            <fanart>resources/fanart.jpg</fanart>
        </assets>
    </extension>
